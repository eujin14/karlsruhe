<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE  mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="board">

<insert id="create" parameterType="hashMap">
<![CDATA[ INSERT INTO board (btitle, bcontent, bwriter, bcategory, bimage, bcount) VALUES (#{btitle}, #{bcontent}, #{bwriter}, #{bcategory},#{bimage}, 0) ]]>
</insert>
<select id="readlist" resultType="hashMap">
<![CDATA[ SELECT * FROM board WHERE bcategory IS NOT NULL ORDER BY bno DESC ]]>
</select>
<select id="readdetail" parameterType="String" resultType="hashMap"> 
<![CDATA[ SELECT * FROM board WHERE bno = #{bno} ]]>
</select>
<update id="update" parameterType="hashMap">
<![CDATA[ UPDATE board SET btitle = #{btitle}, bcontent = #{bcontent}, bwriter = #{bwriter}, bcategory = #{bcategory}, bimage = #{bimage} WHERE bno = #{bno} ]]>
</update>
<delete id="delete" parameterType="String">
<![CDATA[ DELETE FROM board WHERE bno = #{bno} ]]>
</delete>
<update id="count" parameterType="hashMap">
<![CDATA[ UPDATE board SET bcount = bcount +1 WHERE bno = #{bno} ]]>
</update>

